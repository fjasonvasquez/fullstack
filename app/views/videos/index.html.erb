<h1>All Videos</h1>
<hr/>

<%= will_paginate %>

<% @videos.each do |video| %>
	<div class="row">

		<div class="col-lg-8 well">
			<h4><%= link_to video.title, video_path(video) %></h4>
			<p>
				<span class='quiet'><small> <%= time_ago_in_words(video.created_at) %> ago</small></span>
			</p>
		</div>

	</div>
<% end %>

<%= will_paginate %>